<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>DataBox</servlet-name>
		<servlet-class>com.sudheer.assignment3.DataBoxServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Root</servlet-name>
		<servlet-class>com.sudheer.assignment3.RootServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>FileUploadServet</servlet-name>
		<servlet-class>com.sudheer.assignment3.FileUploadServet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>DataBox</servlet-name>
		<url-pattern>/databox</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Root</servlet-name>
		<url-pattern>/</url-pattern>
		<url-pattern>/root</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>FileUploadServet</servlet-name>
		<url-pattern>/upload</url-pattern>
	</servlet-mapping>
	<!-- <welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list> -->
</web-app>
